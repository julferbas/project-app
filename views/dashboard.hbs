<div class="dashboard">
  <div class="review-control">
    <h5>Hi {{user.username}}!</h5>
    {{#each reviews}}
    <div class="each-review">
      <div>
        <h4>Your reviews:</h4><br><br>
        <h6>{{restaurantName}}</h6><br>
        <p>{{review}}</p>
        <br>
        posted on {{date}}:
      </div>
      <a href="/dashboard/review/{{_id}}">Delete</a>
    </div>
    {{else}}
    <p>You have 0 reviews!</p>
    {{/each}}
  </div>

</div>